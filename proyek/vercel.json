{
    "builds" : [{
        "src" : "proyek/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.12"}
    }],
    "routes":[
        {
        "src":"/(.*)",
        "dest":"proyek/wsgi.py"
        }
    ],
    "rewrites": [
          { "source": "/(.*)", "destination": "/" }
    ]
      
}